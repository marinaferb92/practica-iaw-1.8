# Aumentar max_input_vars para Moodle
php_value max_input_vars 5000

# Configuraciones de reescritura u otras configuraciones de Apache si las necesitas
<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteBase /
    RewriteRule ^index\.php$ - [L]
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule . /index.php [L]
</IfModule>